{
  "name": "paviliondev/spinupwp-composer-site",
  "keywords": [
    "WordPress",
    "Composer",
    "SpinupWP"
  ],
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Pavilion",
      "homepage": "https://thepavilion.io/"
    }
  ],
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "git",
      "url": "https://github.com/discourse/wp-discourse.git"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "php": ">=7.1",
    "composer/installers": "^1.4",
    "johnpbloch/wordpress": "5.*",
    "oscarotero/env": "^1.1.0",
    "vlucas/phpdotenv": "^3.0",
    "wpackagist-plugin/limit-login-attempts-reloaded": "*",
    "wpackagist-plugin/spinupwp": "*",
    "wpackagist-theme/twentynineteen": "*",
    "discourse/wp-discourse": "*"
  },
  "extra": {
    "wordpress-install-dir": "public/wp",
    "installer-paths": {
      "public/content/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "public/content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "public/content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "autoload": {
    "psr-4": {
      "DeliciousBrains\\SpinupWPComposerSite\\": "app/"
    }
  }
}